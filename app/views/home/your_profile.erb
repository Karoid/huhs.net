<script type="text/javascript">
	var numberArray = document.getElementsByClassName('number');
	for(var i=0, t=-3; i<numberArray.length; i++)
	{
		var number = numberArray[i];
		number.style.top = t + '%';
		t += 25;
	}

	var lineArray = document.getElementsByClassName('line');
	for(var i=0, t=0; i<lineArray.length; i++)
	{
		var line = lineArray[i];
		line.style.top = t + '%';
		t += 25;
	}

	var graphMAXHeight = document.getElementById('graph_right').clientHeight;
	var graphStickArray = document.getElementsByClassName('stick');
	for(var i=0, left=16; i<graphStickArray.length; i++)
	{
		var graphStick = graphStickArray[i];
		graphStick.style.left = left + 'px';
		left += 58;
		graphStick.style.height = graphStick.getAttribute('value')*graphMAXHeight/400 + 'px';
	}

	var monthArray = document.getElementsByClassName('month');
	for(var i=0, left=57; i<monthArray.length; i++)
	{
		var month = monthArray[i];
		month.style.left = left + 'px';
		left += 58;
	}
	$.urlParam = function(name, url){
		var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(url);
		return results[1] || 0;
	}
	$(document).ready(function() {
		var i = 0
		$(".tap_button").each(function(index, el) {
			if($.urlParam('usage',window.location.href) == $.urlParam('usage',this.href)){
				$(this).addClass('button_clicked')
			}else{
				$(this).removeClass('button_clicked')
			}
		})
	});
</script>
